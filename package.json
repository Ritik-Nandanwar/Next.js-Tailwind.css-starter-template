{
  "name": "tailwind-nextjs-starter-blog",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "start": "next-remote-watch ./data",
    "dev": "next dev",
    "build": "next build && node ./scripts/generate-sitemap",
    "serve": "next start",
    "analyze": "env ANALYZE=true next build"
  },
  "dependencies": {
    "@next/bundle-analyzer": "^10.1.3",
    "@next/mdx": "10.1.3",
    "@tailwindcss/forms": "^0.3.2",
    "@tailwindcss/typography": "^0.4.0",
    "autoprefixer": "^10.2.5",
    "next": "10.1.3",
    "next-mdx-remote": "^2.1.3",
    "next-seo": "4.23.0",
    "postcss": "^8.2.8",
    "preact": "^10.5.13",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "tailwindcss": "^2.1.1"
  },
  "devDependencies": {
    "autoprefixer": "^10.2.5",
    "postcss": "^8.2.9",
    "tailwindcss": "^2.1.1"
  },
  "lint-staged": {
    "*.+(js|jsx|ts|tsx)": [
      "eslint --fix"
    ],
    "*.+(js|jsx|ts|tsx|json|css|md|mdx)": [
      "prettier --write"
    ]
  }
